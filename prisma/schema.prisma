generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}

datasource db {
  provider   = "postgresql"
  url        = env("DATABASE_URL")
  directUrl  = env("DIRECT_URL")
}



model FormSubmission {
  id          Int      @id @default(autoincrement())
  name        String
  email       String   @unique
  branch      String
  reg         String   @unique
  phone       String
  priority    String
  reason      String
  domain      String
  bestProject String
  createdAt   DateTime @default(now())
}

